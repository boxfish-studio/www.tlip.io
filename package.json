{
    "name": "www.tlip.io",
    "version": "0.0.1",
    "description": "TLIP web site",
    "keywords": [
        "tlip",
        "api"
    ],
    "authors": [
        "Martyn Janes <martyn.janes@iota.org>"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tmea-tlip/www.tlip.io.git"
    },
    "license": "Apache-2.0",
    "engines": {
        "node": ">=14",
        "npm": ">=7"
    },
    "scripts": {
        "dev": "svelte-kit dev",
        "build-clean": "rimraf .svelte-kit",
        "build-format": "prettier --config .prettierrc --write .",
        "build-lint": "eslint src",
        "build-check": "svelte-check --tsconfig ./tsconfig.json",
        "build-compile": "svelte-kit build",
        "build-p": "run-p build-lint build-style-lint build-check",
        "build-style-lint": "stylelint ./src/**/*.{scss,svelte}",
        "test-p": "",
        "dist": "run-s build-clean build-format build-p build-compile",
        "preview": "svelte-kit preview",
        "deploy-build": "run-s build-compile"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "^1.0.0-next.13",
        "@sveltejs/kit": "^1.0.0-next.122",
        "@tailwindcss/forms": "^0.3.3",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "autoprefixer": "^10.2.5",
        "cssnano": "^5.0.6",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.4.1",
        "eslint-plugin-svelte3": "^3.2.0",
        "eslint-plugin-unicorn": "^34.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.3.1",
        "postcss-load-config": "^3.0.1",
        "postcss-nested": "^5.0.5",
        "prettier": "^2.3.2",
        "prettier-plugin-svelte": "^2.3.1",
        "rimraf": "^3.0.2",
        "sass": "^1.34.1",
        "stylelint": "^13.13.1",
        "stylelint-config-property-sort-order-smacss": "^7.1.0",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-sass-guidelines": "^8.0.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.19.0",
        "svelte": "^3.38.3",
        "svelte-check": "^2.2.0",
        "svelte-preprocess": "^4.7.4",
        "tailwindcss": "^2.2.4",
        "tslib": "^2.0.0",
        "typescript": "^4.0.0"
    },
    "type": "module"
}
